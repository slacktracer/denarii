{
  "author": "slacktracer",
  "dependencies": {
    "@jest/globals": "^29.2.2",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "pg-mem": "^2.6.3",
    "pg-promise": "^10.12.1",
    "prettier": "^2.7.1",
    "redis": "^4.3.1",
    "redis-memory-server": "^0.6.0",
    "squel": "^5.13.0",
    "supertest": "^6.3.1",
    "uuid": "^9.0.0",
    "vitest": "^0.29.2"
  },
  "license": "MIT",
  "name": "denarii-test-suites",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache --fix .",
    "lint-staged": "lint-staged",
    "save-test-output": "NODE_OPTIONS=--experimental-vm-modules jest --no-color --runInBand --setupFiles dotenv/config --silent --testSequencer ./classes/custom-sequencer.js --verbose 2> test-output.txt",
    "start": "nodemon",
    "test": "chmod +x ./scripts/run-integration-tests.sh && ./scripts/run-integration-tests.sh",
    "test-once": "NODE_OPTIONS=--experimental-vm-modules jest --setupFiles dotenv/config",
    "test-once-on-ci": "prisma migrate dev --name init --schema ../main/prisma/schema.prisma && prisma generate --schema ../main/prisma/schema.prisma && vitest --config ./vitest.config.integration.ts",
    "test-silently": "NODE_OPTIONS=--experimental-vm-modules jest --setupFiles dotenv/config --silent --watch"
  },
  "type": "module",
  "version": "1.0.0"
}
