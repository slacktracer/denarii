{
  "dependencies": {
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3"
  },
  "name": "denarii",
  "scripts": {
    "build": "chmod +x ./build.sh && ./build.sh",
    "d": "npm run decompile --workspace main",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "start": "npm start --workspace main",
    "test": "npm test --workspace tests",
    "test-on-ci": "npm run test-on-ci -w tests",
    "x": "node ./build/main.js"
  },
  "workspaces": [
    "main",
    "tests"
  ]
}
